<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!-- Missing SDK: net47, net471 and net48 -->
    <__NETFrameworks Condition=" $(__Deep) != 'true' ">net452;net462;net472</__NETFrameworks>
    <__NETFrameworks Condition=" $(__Deep) == 'true' ">net452;net46;net461;net462;net472</__NETFrameworks>

    <__NETCoreApps Condition=" $(__Deep) != 'true' ">netcoreapp2.2;netcoreapp3.1</__NETCoreApps>
    <__NETCoreApps Condition=" $(__Deep) == 'true' ">netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netcoreapp3.0;netcoreapp3.1</__NETCoreApps>

    <TargetFrameworks>$(__NETFrameworks);$(__NETCoreApps)</TargetFrameworks>

  </PropertyGroup>

</Project>

<Project Sdk="Microsoft.NET.Sdk">

  <!--
    Blank project used by pack.ps1 to populate the local NuGet cache; see
    NuGet.Config in parent directory.
    We restore packages to the local NuGet cache; see the property
    RestorePackagesPath below.
    https://docs.microsoft.com/en-us/nuget/consume-packages/package-restore#force-restore-from-package-sources
  -->

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <RestorePackagesPath>$(MSBuildThisFileDirectory)../../__/nuget-cache/</RestorePackagesPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Abc.Maybe" Version="$(AbcVersion)" />
  </ItemGroup>

</Project>

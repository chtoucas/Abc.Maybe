<Project Sdk="Microsoft.NET.Sdk">

    <!-- Missing SDKs: net47, net471 and net48 -->

  <PropertyGroup Condition=" $(__Max) == 'true' ">
    <__NETFrameworks>net452;net46;net461;net462;net472</__NETFrameworks>
    <__NETCoreApps>netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netcoreapp3.0;netcoreapp3.1</__NETCoreApps>
  </PropertyGroup>

  <PropertyGroup Condition=" $(__Max) != 'true' ">
    <__NETFrameworks>net452;net462;net472</__NETFrameworks>
    <__NETCoreApps>netcoreapp2.2;netcoreapp3.1</__NETCoreApps>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>$(__NETFrameworks);$(__NETCoreApps)</TargetFrameworks>
  </PropertyGroup>

</Project>
